extern void printInt(int val);
extern void printNewlines(int num);
extern void printSpaces(int num);



export int test(int i) {
    if (i == 0)
        i = 2;

    return i * factorial(i - 1);
}

int factorial(int i) {
    if (i == 0)
        return 1;

    return i * factorial(i - 1);
}


export int main() {
    for (int i = 0, 6) {
        printInt(factorial(i));
        printSpaces(1);
    }
    printNewlines(1);

    return 0;
}
